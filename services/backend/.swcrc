{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript"
		},
		"baseUrl": "./src",
		"paths": {
			"@/*": ["./*"]
		},
		"target": "esnext",
		"loose": false,
		"externalHelpers": false,
		"keepClassNames": false
	},
	"module": {
		"type": "commonjs",
		"strict": true,
		"strictMode": true
	},
	"sourceMaps": true,
	"minify": false
}
